{
  "name": "ecofund-icp-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create ecofund-icp-backend && dfx generate ecofund-icp-backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "generate": "npm run start"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/auth-client": "^2.1.2",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/ledger-icp": "^2.6.2",
    "@dfinity/nns": "^7.0.2",
    "@dfinity/principal": "^2.1.3",
    "@dfinity/utils": "^2.6.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^22.7.5",
    "carbon-components-svelte": "^0.85.0",
    "carbon-icons-svelte": "^12.10.0",
    "carbon-pictograms-svelte": "^12.11.0",
    "carbon-preprocess-svelte": "^0.11.3",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-environment": "^1.1.3"
  }
}
